import java.util.Scanner;

public class Task25 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int input = scanner.nextInt();
		int factorial = 1;
		int count = 0;
		
		do {
			factorial *= ++factorial;
			count++;
		} while (count < input);
		System.out.println(factorial);
	}

}
