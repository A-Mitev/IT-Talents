package exam;

import exceptions.InvalidCityException;
import exceptions.InvalidNameExeption;
import exceptions.InvalidPersonalNumberException;

public class ObsluzvanClient extends Person{

	@SuppressWarnings("unused")
	private Location obsluzvashtOffice;

	public ObsluzvanClient(String name, String tel, City address, String personalNumber, Location obsluzvashtOffice)
			throws InvalidNameExeption, InvalidCityException, InvalidPersonalNumberException {
		super(name, tel, address, personalNumber);
		if(obsluzvashtOffice != null){
		this.setObsluzvashtOffice(obsluzvashtOffice);
		}
	}

	public void setObsluzvashtOffice(Location obsluzvashtOffice) {
		if(obsluzvashtOffice != null){
		this.obsluzvashtOffice = obsluzvashtOffice;
		}
	}
	
	

}
