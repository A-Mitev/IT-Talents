public class Simple_Task_1 {
	public static int fibonachiFunc(int num, int input, int count){
		if(count == input){
			return num;
		}
		num += num;
		return fibonachiFunc(num, input, count);
		
	}
	
	public static void main(String[] args) {
		int num = 1;
		int input = 8;
		int count = 0;
		
		System.out.println(fibonachiFunc(num, input, count));
	}

}
