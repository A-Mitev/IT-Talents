import java.util.Scanner;

public class Class_Examples {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		String a = "test";
		String b = "test";
		
		//String input = scanner.nextLine();
		//String inputNext = scanner.next();
		
		//System.out.println(input);
		//System.out.println(inputNext);
		
		// vazno za sravnenie se polzva .equals ili compareTo
		if (a.equals(b)){
		System.out.println(a);
		} else {
			System.out.println("Ne sa ednakvi");
		}
		
		// vazno za dulzinata e s skobi
		System.out.println(a.length());
		
		char firstLetter = a.charAt(0);
		System.out.println(firstLetter);
	}

}
