import java.util.Scanner;

public class Task20 {
	public static void main(String[] args) {

		for (int i = 0; i <= 9; i++) {
			for (int j = 0; j < 10; j++) {
				if ((j + i) > 9) {
					j = 0;
					System.out.println(j);
					continue;
				}
				System.out.print(j + i);

			}
			System.out.println();

		}

	}
}
