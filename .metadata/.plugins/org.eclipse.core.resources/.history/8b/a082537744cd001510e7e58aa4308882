package Collection;

public class Koli {
	
	private int doors;
	private String fuel;
	private int prise;
	
	public Koli(int doors, String fuel, int price){
		setDoors(doors);
		setFuel(fuel);
		setPrice(price);
	}
	
	
	public void setDoors(int doors) {
		this.doors = doors;
	}
	
	public void setFuel(String fuel){
		this.fuel = fuel;
	}
	
	public void setPrice(int price){
		this.prise = price;
	}
	
	
	
	

}
