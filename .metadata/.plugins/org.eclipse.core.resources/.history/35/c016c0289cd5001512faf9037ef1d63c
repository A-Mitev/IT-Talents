package ThreadsTest;

public class MyFirstThread implements Runnable{

	@Override
	public void run() {
		
		while(true){
			try {
				Thread.sleep(500);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		System.out.println("Tova e moqta purva nishka");
		}
	}
	
	
}
