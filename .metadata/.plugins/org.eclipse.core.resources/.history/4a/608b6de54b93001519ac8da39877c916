public class Simple_Task_1 {
	public static int fibonachiFunc(int input, int count){
		int num = 1;
		if(count == input){
			return num;
		}
		num += num;
		return num + fibonachiFunc(num, count);
		
	}
	
	public static void main(String[] args) {
		int input = 8;
		int count = 0;
		
		System.out.println(fibonachiFunc(input, count));
	}

}
