public class Task1 {
	public static void main(String[] args) throws IOException {
		
		File dir = new File("iotest");
		if(dir.isDirectory()){
			System.out.println("Ima q nqma da suzdavam");
		} else {
			dir.mkdir();
		}
		
		File newFile = new File("dir\\test.txt");
		newFile.createNewFile();
		
	}

}
