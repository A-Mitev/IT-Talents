package test;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;

public class TestDBConnection {

	public static void main(String[] args) throws SQLException {
		
		
		
		Connection connect = DriverManager.getConnection("jdbc:mysql://localhost:3306/Peoples?autoReconnect=true&useSSL=false");
		
		Statement statement = connect.createStatement();
		ResultSet result = statement.executeQuery("select * from mydb.Cart");
		
		while(result.next()){
			System.out.println(result.getInt("Broiki"));
		}
		
	}
}
