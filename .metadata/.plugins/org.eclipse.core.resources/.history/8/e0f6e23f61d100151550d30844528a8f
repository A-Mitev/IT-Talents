package Task3Scheduler;

public class Task implements ITask {

	private String Name;
	private static int id = 0;
	private int number;

	public Task(String name) {
		setName(name);
		id++;
		number = id;

	}

	public String getName() {
		return Name;
	}

	public void setName(String name) {
		if (name != null && !name.equals(""))
			Name = name;
	}

	@Override
	public void doWork() {

	}

}
