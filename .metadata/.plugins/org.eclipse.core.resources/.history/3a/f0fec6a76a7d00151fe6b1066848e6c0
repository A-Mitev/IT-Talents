//		Task 6;
		
		int num = scanner.nextInt();
		String binary = Integer.toBinaryString(num);
		int binaryNum = Integer.parseInt(binary);
		int countZero = 0;
		int countOne = 1;
		
		for (int i = 0; i < binary.length(); i++) {
			int bit = binaryNum & 1;
			if (bit == 1){
				countOne ++;
			} else {
				countZero ++;
			}
			binaryNum = binaryNum >> i;
		}
	}
}
