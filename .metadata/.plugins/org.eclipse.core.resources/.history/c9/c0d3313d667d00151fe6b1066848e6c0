import java.util.Scanner;

public class Task19 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int num = scanner.nextInt();
		
		if ((num < 10)&& (num > 99)){
			System.out.println("Wrong input!");
			num = scanner.nextInt();
		}
		
		for (int i = num; i >= 1; i--) {
			if (--i % 2 == 0){
				System.out.println((i * 3) + 1 );
				
			} else {
				System.out.println(num * 0.5);
			}
		}
		
	}

}
