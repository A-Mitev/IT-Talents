import java.util.Scanner;

public class Task_02 {
	public static void main(String[] args) {
		@SuppressWarnings("unused")
		Scanner scanner = new Scanner(System.in);

		String firstWord = "uchilishe";
		String secondWord = "uchenik";
		String newFirstWord = "";                                              // Inicializiram prazni stringove ot koito da polucha krajnata duma.
		String newSecondWord = "";

		int longerWord = Math.max(firstWord.length(), secondWord.length());   // Opredelqm dulzinata na po dulgata duma.
		int symbolsForChange = 5;

		if (firstWord.length() == longerWord) {                               // Ako purvata duma e po - dulgata ...
			// S pomoshtta na .substring vzimam samo neobhodimite mi simvoli. 
			newFirstWord = secondWord.substring(0, 6) + firstWord.substring(6, firstWord.length()); 
			System.out.println(longerWord + " " + newFirstWord);
		} else {
			if (firstWord.length() == longerWord) {
				newSecondWord = firstWord.substring(0, 6) + secondWord.substring(6, secondWord.length());
				System.out.println(longerWord + " " + newSecondWord);
			}
		}
	}
}
