package Task3Scheduler;

public class Task implements ITask {

	private String work;
	
	
	
	public Task(String work) throws WrongWorkEntryExceprion {
		setWork(work);
	}



	public void setWork(String work) throws WrongWorkEntryExceprion {
		if(work != null && !work.equals("") && work.length() > 0){
		this.work = work;
		} else {
			throw new WrongWorkEntryExceprion("Invalid work entry!");
		}
	}



	@Override
	public void doWork() {
		System.out.println(work);
	}

	

	

}
