import java.util.Scanner;

public class Task24 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int input = scanner.nextInt();
		int rev = 0;
		int digit = 0;
		do {
		digit = input % 10;
	      rev = rev * 10 + digit;
	      input = input / 10;
	} while (input > 0);
		
		if ( input == rev){
			System.out.println("Palindrome");
		}
	}

}
