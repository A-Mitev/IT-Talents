package Task_02;
public class Call {
	
	private double priceForAMinute;
	GSM caller;
	GSM receiver;
	int duration;
	
	Call(double price){
		this.priceForAMinute = price;
	}
	
	public double getPriceForAMinute() {
		return priceForAMinute;
	}

	public void setPriceForAMinute(double priceForAMinute) {
		
		this.priceForAMinute = priceForAMinute;
	}
}
