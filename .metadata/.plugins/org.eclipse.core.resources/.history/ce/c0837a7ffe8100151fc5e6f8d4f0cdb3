import java.sql.Date;
import java.text.SimpleDateFormat;
import java.util.Scanner;

public class Task12 {
	public static void main(String[] args) {
		@SuppressWarnings("resource")
		Scanner scanner = new Scanner(System.in);

		int day = scanner.nextInt();
		int month = scanner.nextInt();
		int year = scanner.nextInt();
		boolean leapYear = (year % 4 == 0);
		boolean wrongDate = false;

		while ((day < 0 || day > 31) || (month < 0 || month > 12) || (year < 0)) {
			System.out.println("Invalid input, please repeat with correct data!");
			day = scanner.nextInt();
			month = scanner.nextInt();
			year = scanner.nextInt();
		}

		if (year % 100 == 0) {
			if (year % 400 == 0) {
				leapYear = true;
			} else {
				leapYear = false;
			}
		}

		if (day == 29 && month == 2 && leapYear) {
			month++;
			day = 1;
		} else

		if (day == 28 && month == 2 && !leapYear) {
			month++;
			day = 1;
		} else

		if (month == 2 && day > 29) {
			wrongDate = true;
		} else

		if (day == 30) {
			switch (month) {
			case 1:
			case 3:
			case 5:
			case 7:
			case 8:
			case 10:
			case 12:
				day++;
				break;
			case 4:
			case 6:
			case 9:
			case 11:
				day = 1;
				if (month == 12) {
					month = 1;
					year++;
				} else {
					month++;
				}
				;
				break;
			}
		} else if (day == 31) {
			day = 1;

			if (month == 12) {
				month = 1;
				year++;
			} else {
				month++;
			}
		} else {
			day++;
		}

		if (!wrongDate) {
			System.out.println(day + " " + month + " " + year);
		} else {
			System.out.println("THe date entered is wrong!");
		}

	}
}