import java.util.Scanner;

public class Task_02 {
	public static void main(String[] args) {
		@SuppressWarnings("unused")
		Scanner scanner = new Scanner(System.in);

		String firstWord = "uchilishe";
		String secondWord = "uchenik";
		String newFirstWord = "";
		String newSecondWord = "";

		int longerWord = Math.max(firstWord.length(), secondWord.length());
		int symbolsForChange = 5;

		if (firstWord.length() == longerWord) {
			newFirstWord = secondWord.substring(0, 4) + firstWord.substring(5, firstWord.length());
			System.out.println(longerWord + " " + newFirstWord);
		} else {
			if (firstWord.length() == longerWord) {
				newSecondWord = firstWord.substring(0, 4) + secondWord.substring(5, secondWord.length());
				System.out.println(longerWord + " " + newSecondWord);
			}
		}
	}
}
