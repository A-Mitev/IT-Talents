import java.util.Scanner;

public class Task13 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);

		int number = 150;
		int binary = 0;
		while (number >= 0) {
			number /= 2;
			binary += number % 2;
			
//			if (number != 0) {
//				binary *= 10;
//			}
		}
		System.out.println(binary);
	}

}
