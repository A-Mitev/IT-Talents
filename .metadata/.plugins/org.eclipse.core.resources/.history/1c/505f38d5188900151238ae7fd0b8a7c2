import java.util.Scanner;

public class Task_02 {
	public static void main(String[] args) {
		@SuppressWarnings("unused")
		Scanner scanner = new Scanner(System.in);

		String firstWord = "uchilishe";
		String secondWord = "uchenik";
		String newFirstWord = "";
		String newSecondWord = "";

		int longerWord = Math.max(firstWord.length(), secondWord.length());
		int symbolsForChange = 5;

		if (firstWord.length() == longerWord) {
			for (int i = 0; i < symbolsForChange; i++) {
				newFirstWord = firstWord.replace(firstWord.charAt(i), secondWord.charAt(i));
			}
			System.out.println(longerWord + " " + firstWord);
		} else {
			if (firstWord.length() == longerWord) {
				for (int i = 0; i < symbolsForChange; i++) {
					newSecondWord = firstWord.replace(firstWord.charAt(i), secondWord.charAt(i));
				}
				System.out.println(longerWord + " " + newSecondWord);
			}
		}
	}
}
