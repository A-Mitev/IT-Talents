package Task1Bank;

import java.util.List;

public class Client {

	private String name;
	private String address;
	private double moneyAmounth;
	private double salary;
	private List<BankProduct> clientProducts;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		if (name != null && !name.equals(""))
			this.name = name;
	}

	public String getAddress() {
		return address;
	}

	public double getMoneyAmounth() {
		return moneyAmounth;
	}

	public double getSalary() {
		return salary;
	}

	public void setSalary(double salary) {
		if (salary > 0) {
			this.salary = salary;
		}
	}

}
