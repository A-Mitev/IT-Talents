package EmployeeTask;

public class COmpanlyException extends Exception {
	
	
	

	public COmpanlyException() {
		// TODO Auto-generated constructor stub
	}

	public COmpanlyException(String arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}

	public COmpanlyException(Throwable arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}

	public COmpanlyException(String arg0, Throwable arg1) {
		super(arg0, arg1);
		// TODO Auto-generated constructor stub
	}

	public COmpanlyException(String arg0, Throwable arg1, boolean arg2, boolean arg3) {
		super(arg0, arg1, arg2, arg3);
		// TODO Auto-generated constructor stub
	}

}
