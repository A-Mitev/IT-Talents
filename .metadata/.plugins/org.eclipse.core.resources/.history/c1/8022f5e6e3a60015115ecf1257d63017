package RandomTests;

public class CarDemo {
	public static void main(String[] args) {

		Car skoda = new Car("Skoda", true, "silver");
		Car audi = new Car("Audi", false, "red");
		
		skoda.maxSpeed = 220;
		skoda.gears = 5;
		
		Person alex = new Person();
		alex.name = "Alex";
		alex.age = 29;
		
		skoda.owner = alex;
		
		audi.printCarDetails(audi);
		
		
		skoda.printOwnerDetails(alex);
	}
}
