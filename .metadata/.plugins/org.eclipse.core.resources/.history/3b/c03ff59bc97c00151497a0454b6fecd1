import java.util.Scanner;

public class Task11 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int num = scanner.nextInt();
		String symbol = "*";
		
		
		for (int i=1; i<10; i += 2)
		{
		    for (int j=0; j<i; j++)
		    {
		        System.out.print("*");
		    }
		    System.out.println("");
		}
	}

}
