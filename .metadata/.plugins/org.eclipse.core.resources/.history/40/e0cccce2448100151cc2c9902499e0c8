		int[] a = new int[15];
		int max = Integer.MIN_VALUE;
		int min = Integer.MAX_VALUE;
		int multiplyAllEven = 1;
		
		for (int i = 0; i < a.length; i++) {
			a[i]= scanner.nextInt() ;
		}
		
		for (int i = 0; i < a.length; i++) {
			if (a[i] > max){
				max = a[i];
			}
		}
		
		for (int i = 0; i < a.length; i++) {
			if (a[i] < min){
				min = a[i];
			}
		}
		
		for (int i = 0; i < a.length; i++) {
			if (i % 2 == 0){
				multiplyAllEven *= i;
			}
		}
		System.out.println("Max = " + max);
		System.out.println("Min = " + min);
		System.err.println("Multiply all EVEN members of the array = " + multiplyAllEven);
	}
}
