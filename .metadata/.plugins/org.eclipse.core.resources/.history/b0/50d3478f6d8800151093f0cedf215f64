import java.util.Scanner;

public class Class_Examples {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		String a = "  test";
		String b = "test";
		
		//String input = scanner.nextLine();
		//String inputNext = scanner.next();
		
		//System.out.println(input);
		//System.out.println(inputNext);
		
		// vazno za sravnenie se polzva .equals ili compareTo
		if (a.equals(b)){
		System.out.println(a);
		} else {
			System.out.println("Ne sa ednakvi");
		}
		
		// vazno za dulzinata e s skobi
		System.out.println(a.length());
		
		// vzimane na konkreten simvol, zapochva ot 0
		char firstLetter = a.charAt(3);
		System.out.println(firstLetter);
		System.out.println();
		
		for (int i = 0; i < a.length(); i++) {
			System.out.println(a.charAt(i));
		}
		
		//na koq poziciq e daden simvol
		System.out.println(a.indexOf("e"));
		System.out.println(a.lastIndexOf("es"));
		
		
	}

}
