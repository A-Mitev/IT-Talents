public class Task02 {
	public static void main(String[] args) {
		
		int[][] array = {
							{1,4,6,3},
							{5,9,7,2},
							{4,8,1,9},
							{2,3,4,5},
						};
		
		for (int i = 0; i < array.length; i++) {
			for (int j = 0; j < 1; j++) {
				System.out.println(array[i][j]);
				i++;
				j++;
			}
		}
		
	}

}
