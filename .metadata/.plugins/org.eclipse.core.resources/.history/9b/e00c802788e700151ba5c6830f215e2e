package exam;

public class InvalidNameExeption extends Exception {
	
	
	private static final long serialVersionUID = 2460077089371358000L;

	public InvalidNameExeption() {
	}

	public InvalidNameExeption(String arg0) {
		super(arg0);
	}

	public InvalidNameExeption(Throwable arg0) {
		super(arg0);
	}

	public InvalidNameExeption(String arg0, Throwable arg1) {
		super(arg0, arg1);
	}

	public InvalidNameExeption(String arg0, Throwable arg1, boolean arg2, boolean arg3) {
		super(arg0, arg1, arg2, arg3);
	}
}
