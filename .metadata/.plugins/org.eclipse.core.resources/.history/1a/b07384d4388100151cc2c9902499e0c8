import java.util.Scanner;

public class Arrays_Class_Tasks {
	public static void main(String[] args) {
		@SuppressWarnings("resource")
		Scanner scanner = new Scanner(System.in);
		
		int[] a = new int[10];
		
//		for (int i = 0; i < a.length; i++) {
//			a[i] = scanner.nextInt();
//		}
//		
//		for (int i = 0; i < a.length; i++) {
//			System.out.print(a[i] + " ");
//		}
		
		for (int i = 0; i < a.length; i++) {
			a[i] = scanner.nextInt();
		}
		int sumFirst = 0;
		int sumSecond = 0;
		int multiplyFirst = 0;
		int multiplySecond = 0;
		
		for (int i = ((a.length)/ 2) - 1; i > -1; i--) {
			System.out.print(a[i] + ",");
			sumFirst += a[i];
			multiplyFirst *= a[i];
			
		}
		System.out.print("  ");
		
		for (int i = a.length -1; i > (a.length /2) -1  ; i--) {
			System.out.print(a[i] + ",");
			sumSecond += a[i];
			multiplySecond *= a[i];
		}
		
		System.out.println(sumFirst + " " + multiplyFirst);
		System.out.println(sumSecond + " " + multiplySecond);
		
	}
}
